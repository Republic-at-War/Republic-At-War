				<!-- We can block/redirect shots from the following units types: -->
				<Applicable_Unit_Categories>Infantry</Applicable_Unit_Categories>
				<Applicable_Unit_Types>Aayla_Secura, Anakin_Skywalker, Barris_Offee, Commander_Cody, Boss, Fixer, Sev, Scorch, Luminara_Unduli, Mace_Windu, Obiwan, Plokoon, Shaak_Ti, Yoda, Clone_Commando, Arc_Trooper, Arc_Trooper, Arc_Trooper_Captain, Arc_Trooper_Z6, Generic_Field_Commander_P1_Republic, Generic_Field_Commander_P2_Republic</Applicable_Unit_Types>
