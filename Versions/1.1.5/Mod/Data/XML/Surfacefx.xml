<!-- NOTE!!!!   The "name" of the SurfaceFX comes from SurfaceFXTriggerType.xml
				To create a new SurfaceFX, first add new trigger in SurfaceFXTriggerType.xml, 
				and then define it here. -->


<SurfaceEffects>

	<SurfaceFX name="Infantry_Terrain_Modifier">
		
		<Surface_Settings name="default">
			<Dynamic_Track_Name>NONE</Dynamic_Track_Name>
		</Surface_Settings>
		
		<Surface_Settings name="good_ground">
			<Defense_Mod>0.5</Defense_Mod> <!-- 50% taken damage reduction -->
			<Damage_Mod>1.0</Damage_Mod>	<!-- 100% caused damage increase -->
         <Status_Icon_Name>i_surface_good.tga</Status_Icon_Name>
		</Surface_Settings>

		<Surface_Settings name="high_ground">
			<FOW_Reveal_Range_Mod>0.25</FOW_Reveal_Range_Mod> <!-- 25% farther view -->
			<Fire_Range_Mod>0.25</Fire_Range_Mod>	<!-- 25% farther shot range -->
         <Status_Icon_Name>i_surface_high.tga</Status_Icon_Name>
		</Surface_Settings>

		<Surface_Settings name="slow_ground">
         <Speed_Mod>-0.5</Speed_Mod>
         <Status_Icon_Name>i_surface_slow.tga</Status_Icon_Name>
		</Surface_Settings>

		<Surface_Settings name="water">
         <Speed_Mod>-0.5</Speed_Mod>
         <Status_Icon_Name>i_surface_slow.tga</Status_Icon_Name>
		</Surface_Settings>

		<Surface_Settings name="mud">
         <Speed_Mod>-0.5</Speed_Mod>
         <Status_Icon_Name>i_surface_slow.tga</Status_Icon_Name>
		</Surface_Settings>
		
		<Surface_Settings name="damage_ground">
         <Terrain_Damage>10</Terrain_Damage>
         <Terrain_Damage_Delay>1.0</Terrain_Damage_Delay>
         <Status_Icon_Name>i_surface_damage.tga</Status_Icon_Name>
		</Surface_Settings>

		<Surface_Settings name="lava">
         <Terrain_Damage>10</Terrain_Damage>
         <Terrain_Damage_Delay>1.0</Terrain_Damage_Delay>
         <Status_Icon_Name>i_surface_damage.tga</Status_Icon_Name>
		</Surface_Settings>

		<Surface_Settings name="infantry_ground">
